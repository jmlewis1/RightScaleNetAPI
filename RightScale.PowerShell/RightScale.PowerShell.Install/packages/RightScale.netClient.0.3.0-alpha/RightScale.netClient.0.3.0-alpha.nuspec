<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>RightScale.netClient</id>
    <version>0.3.0-alpha</version>
    <title>RightScale .net API Client</title>
    <authors>Patrick McClory</authors>
    <owners>Patrick McClory</owners>
    <projectUrl>https://github.com/patrickmcclory/RightScaleNetAPI</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
